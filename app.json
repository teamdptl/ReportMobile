{
  "expo": {
    "name": "ReportMobile",
    "slug": "ReportMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/SguLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "Cho phép $(PRODUCT_NAME) sử dụng camera."
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.huykhaduy.ReportMobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/SguLogo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.huykhaduy.ReportMobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "702e1dcf-e4cf-4a12-be48-ff3456813b8d"
      }
    }
  }
}
